    "GDRIVE_FOLDER_ID": "{{GDRIVE_FOLDER_ID}}",
    "OPENAI_API_KEY": "{{VERALUX_HOSTED_KEY}}"
  },
  "branding": {
    "logo": "branding/{{COMPANY_ID}}/logo.png",
    "watermark": "branding/{{COMPANY_ID}}/wm.png",
